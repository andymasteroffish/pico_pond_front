        x = padding + rnd(128-padding*2)
        y = y
        circfill(x,y,s, c)
    end

    rectfill(0,0,24,19,0)
    
    sspr(40,0, 24,19, 0,0)

    memcpy(0,sprite_start,8191)
    printh("save it")

end

